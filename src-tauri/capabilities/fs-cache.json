{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "fs-cache",
  "description": "Access to cache directory",
  "windows": ["main"],
  "permissions": [
    {
      "identifier": "fs:scope",
      "allow": [{ "path": "$CACHE/**/*" }]
    },
    "fs:allow-read-file",
    "fs:allow-write-file",
    "core:path:allow-resolve-directory"
  ]
}
